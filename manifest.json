{
  "manifest_version": 3,
  "name": "Prayer Times by Region",
  "version": "1.0",
  "description": "Shows prayer times based on your current region",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["geolocation", "storage"],

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
